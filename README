------------- Rotating text from https://github.com/peachananr/simple-text-rotator -----------
        {% load staticfiles %}
        <script type="text/javascript" src="{% static "js/jquery.simple-text-rotator.js" %}"></script>
        <link rel="stylesheet" href="{% static "css/simpletextrotator.css" %}">
        <script>
        $(document).ready( function(){
                $(".rotate").textrotator({
                animation: "fade", // Options are dissolve (default), fade, spin, flip,  flipUp, flipCube and flipCubeUp.
                speed: 1500
                });
        })
        </script>
        Send some of your <h4 class="rotate">positivity, prana, qi, love</h4> back to {{ instructor.instructor_name }}